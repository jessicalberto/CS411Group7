<html lang ="en">
<head>
  <h2>Displaying the Top 10 Local Bars from your Search:</h2>

  <!--Like ERB, JavaScript between <% %> is executed. JavaScript between <%= %>
  adds HTML to the result. -->

<ul>
  <% businessarray.forEach(function(element) { %>
    <li><%= element %></li>
  <% }); %>
</ul>

</head>
